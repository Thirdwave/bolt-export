    "keywords": [
        "bolt",
        "export"
    ],
    "require": {
        "bolt/bolt": ">=2.0.0,<3.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Thirdwave",
            "email": "info@thirdwave.nl",
